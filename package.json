{
  "name": "ARUConsul",
  "version": "0.1.0",
  "description": "Autonomous development powered by Agentic OS",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "setup-larkbase": "node scripts/setup-larkbase.cjs",
    "update-completion": "node scripts/setup-formula-rate.cjs",
    "setup-multi-tenant": "node scripts/setup-multi-tenant.cjs",
    "generate-tasks": "node scripts/generate-customer-tasks.cjs",
    "check-overdue": "node scripts/check-overdue-tasks.cjs",
    "check-overdue:notify": "node scripts/check-overdue-tasks.cjs --notify",
    "weekly-report": "node scripts/weekly-report.cjs",
    "weekly-report:notify": "node scripts/weekly-report.cjs --notify"
  },
  "keywords": [
    "agentic-os",
    "autonomous"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.56.1",
    "dotenv": "^17.2.3"
  }
}
